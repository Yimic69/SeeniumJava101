<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SeleniumJava101" parallel="tests" thread-count="5">

	<test name="Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="URL"
			value="https://www.lambdatest.com/selenium-playground/." />
		<classes>
			<class name="TestSelenium.TestScenarios"></class>
		</classes>
	</test>

	<test name="Edge">
		<parameter name="browser" value="edge" />
		<parameter name="URL"
			value="https://www.lambdatest.com/selenium-playground/." />
		<classes>
			<class name="TestSelenium.TestScenarios"></class>
		</classes>
	</test>

	<test name="Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="URL"
			value="https://www.lambdatest.com/selenium-playground/." />
		<classes>
			<class name="TestSelenium.TestScenarios"></class>
		</classes>
	</test>
 
	<test name="Internet Explorer">
		<parameter name="browser" value="internetexplorer" />
		<parameter name="URL"
			value="https://www.lambdatest.com/selenium-playground/." />
		<classes>
			<class name="TestSelenium.TestScenarios"></class>
		</classes>
	</test>

</suite>

 
